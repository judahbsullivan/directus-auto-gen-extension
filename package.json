{
  "name": "directus-auto-gen-extension",
  "version": "3.4.2",
  "type": "module",
  "description": "Perform computed value based on other fields",
  "author": {
    "email": "jsullivan@hopemediagoup.com",
    "name": "Judah Sullivan"
  },
  "license": "gpl-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/judahbsullivan/directus-auto-gen-extension.git"
  },
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-interface",
    "directus-custom-hook"
  ],
  "directus:extension": {
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "interface",
        "name": "auto-gen-interface",
        "source": "src/interface/index.ts"
      },
      {
        "type": "hook",
        "name": "auto-gen-hook",
        "source": "src/hook/index.ts"
      },
      {
        "type": "interface",
        "name": "auto-gen-template-editor",
        "source": "src/template-editor/index.ts"
      }
    ],
    "host": "^10.10.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "test": "TZ=UTC jest"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@directus/extensions-sdk": "^17.0.7",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.4.0",
    "vue": "^3.4.0"
  },
  "dependencies": {
    "vue-i18n": "^10.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
